{
  "name": "Jesse's Scrobbler",
  "version": "1.0",
  "description": "Scrobble from streaming music sources!",
  "background_page": "background.html",
    "browser_action": {
    "default_icon": "img/ico.gif",
    "popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":["http://*/*"],
      "js":["javascript/messenger.js"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "notifications",
    "tabs",
    "history",
    "http://*/", 
    "https://*/"
  ]
}